    "generate": "openapi-typescript ../../backend/api/openapi.yaml -o ./src/schema.d.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.3"
  },
  "devDependencies": {
    "@keel/config": "workspace:*",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.3.3"
  }
}
