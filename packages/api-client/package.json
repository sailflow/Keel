{
  "name": "@keel/api-client",
  "version": "0.0.1",
  "private": true,
  "description": "Generated TypeScript client for Keel API",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
