{
  "name": "@keel/api-client",
  "version": "0.0.1",
  "private": true,
  "description": "Generated TypeScript client for Keel API",
  "main": "./main.ts",
  "types": "./main.ts",
  "exports": {
    ".": "./main.ts"
  },
  "scripts": {
    "generate": "openapi-ts",
    "generate:watch": "openapi-ts --watch",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"**/*.gen.ts\" --ignore-pattern \"src/@tanstack/**\" --ignore-pattern \"src/client/**\" --ignore-pattern \"src/core/**\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.19"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.90.6",
    "@keel/config": "workspace:*",
    "openapi-typescript": "^7.10.1",
    "react": "^19.2.3",
    "typescript": "^5.3.3"
  }
}
